
<%= form_with model: @prime_contractor, url:prime_contractors_path, local: true do |f| %>
<%= f.text_field :pc_name, placeholder:"元請名（ 40文字まで)", maxlength:"40" %>
<%= f.text_field :pc_contact_person, placeholder:"元請担当者名（ 40文字まで)", maxlength:"40" %>
<%= f.text_field :pc_telephone, placeholder:"例）09012345678",maxlength:"11"%>
<%= f.submit "登録する" %>
<% end %>