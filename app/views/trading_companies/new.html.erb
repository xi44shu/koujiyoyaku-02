
<%= form_with model: @trading_company, url:trading_companies_path, local: true do |f| %>
<%= f.text_field :tc_name, placeholder:"商社名（ 40文字まで)", maxlength:"40" %>
<%= f.text_field :tc_contact_person, placeholder:"商社担当者名（ 40文字まで)", maxlength:"40" %>
<%= f.text_field :tc_telephone, placeholder:"例）09012345678",maxlength:"11"%>
<%= f.submit "登録する" %>
<% end %>